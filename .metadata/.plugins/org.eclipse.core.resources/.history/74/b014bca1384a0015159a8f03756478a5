package com.basic.learning.bean;

public class Person implements Comparable{
	private String name;
	private int age;
	//constructor
	public Person(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
	
	public Person() {
		super();
	}

	@Override
	public int hashCode() {		
		System.out.println(this+"¡­¡­hashCode");
		return age*39+name.hashCode();
	}

	@Override
	public boolean equals(Object obj) {
		if(this == obj)
			return true;
		if(!(obj instanceof Person))
			throw new ClassCastException("ÀàĞÍ´íÎó");
		Person p = (Person)obj;
		System.out.println(this.name+"¡­¡­equals");
		return this.name.equals(p.name) && (this.age == p.age);
	}
	@Override
	public String toString()
	{
		return this.name+"¡­¡­"+this.age;
	}
	//get & set
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	@Override 
	public int compareTo(Object obj)
	{
		Person p =(Person)obj;
		if(this.age > p.age)
			return 1;
		if(this.age < p.age)
			 return -1;
		else
		{
			/*if(this.name.equals(p.name))
				return 0;
			else
				return 1;*/
			//String.compareTo()£º°´ÕÕ×ÖµäË³Ğò
			System.out.println("±È½Ï²îÖµ£º"+this.name.compareTo(p.name));		
			return this.name.compareTo(p.name);
		}
	}
	
}
