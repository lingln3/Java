
public class CastingNumber {
	public void castingNumber(){
		double above = 0.7, below = 0.4;
		float fabove = 0.7f, fbelow = 0.4f;
		System.out.println("above = " + above + "  " + "below  = " + below);
		System.out.println("fabove = " + fabove + "  " + "fbelow  = " + fbelow);
		System.out.println("(int)above = " + (int)above + "  " + "(int)below  = " + (int)below);
		System.out.println("(int)fabove = " + (int)fabove + "  " + "(int)fbelow  = " + (int)fbelow);
		
	}
}
